{
  "name": "zapp-hierarchy-browser",
  "version": "1.0.0",
  "description": "Component to browse a hierarchical ontology.",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "tsup src/index.ts --dts --format esm,cjs --sourcemap --clean --external react,react-dom,zod",
    "build:demo": "vite build",
    "dev": "vite",
    "preview": "vite preview",
    "test": "vitest"
  },
  "type": "module",
  "author": "Patrick Golden",
  "license": "BSD-3-Clause",
  "peerDependencies": {
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "zod": "^4.0.17"
  },
  "dependencies": {
    "lunr": "^2.3.9",
    "treeverse": "^3.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.24.0",
    "@types/lunr": "^2.3.7",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.1",
    "@types/treeverse": "^3.0.5",
    "@typescript-eslint/eslint-plugin": "^8.29.0",
    "@typescript-eslint/parser": "^8.29.0",
    "@vitejs/plugin-react": "^5.0.4",
    "eslint": "^9.24.0",
    "globals": "^16.0.0",
    "prettier": "^3.5.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "tsup": "^8.5.0",
    "tsx": "^4.19.3",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.30.1",
    "vite": "^7.1.9",
    "vitest": "^3.2.4",
    "zod": "^4.0.17"
  }
}
